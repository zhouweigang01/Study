@{
    ViewBag.Title = "组织管理";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Head{
    <script src="/Areas/Script/Super/SuperConsole/Org.js" type="text/javascript"></script>
}
<div data-options="region:'center'">
    <table id="orgList" class="easyui-datagrid" iconcls="icon-user" data-options="fit:true,border:false">
    </table>
</div>
<!--编辑界面-->
<div id="editForm" class="easyui-dialog" title="新增组织" iconcls="icon-save">
    <table style="padding: 10px;">
        <tr>
            <td class="label-t">
                编码
            </td>
            <td>
                <input id="txtCode" type="text" class="easyui-validatebox" style="width: 180px" />
            </td>
        </tr>
        <tr>
            <td class="label-t">
                名称
            </td>
            <td>
                <input id="txtName" type="text" class="easyui-validatebox" style="width: 180px" />
            </td>
        </tr>
    </table>
</div>
